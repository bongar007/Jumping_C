<p class="welcome lead mt-3 mb-3">Welcome <%= user.name %></p>
<a href="/users/logout" class="btn btn-secondary">Logout</a>

<div id="canvas-container" class="container game-container">
  <div class="progress">
    <div
      class="progress-bar progress-bar-animated active"
      id="dynamic"
      role="progressbar"
      aria-valuemin="0"
      aria-valuenow="0"
      aria-valuemax="10"
    ></div>
    <span id="current-progress"></span>
  </div>
  <div class="row">
    <div class="container col-md-6">
      <span class="best-score"><%= user.highScore %> </span><br />
      <span class="high-score"></span><br />
      <span class="current-score"></span>
    </div>
    <div class="container question-container col-md-6">
      <h4 class="question-header">Question:</h4>
      <p class="question"></p>
    </div>
  </div>
</div>
